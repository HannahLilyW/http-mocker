{
  "name": "http-mocker",
  "displayName": "http-mocker",
  "description": "http mocking tool",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "http-mocker",
          "title": "HTTP Mocker",
          "icon": "icons/mockface.svg"
        }
      ]
    },
    "views": {
      "http-mocker": [
        {
          "type": "webview",
          "id": "http-mocker-enabled",
          "name": "Enable/Disable Mocker"
        },
        {
          "id": "discover-paths",
          "name": "Discover Paths"
        },
        {
          "id": "mock-endpoints",
          "name": "Mock Endpoints"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.83.0",
    "@types/mocha": "^10.0.2",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "eslint": "^8.50.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2",
    "@vscode/test-electron": "^2.3.4"
  }
}
